// 鼠标样式
a {
    cursor: url(https://feiju12138.github.io/cursor/ayuda.cur), auto
}
a:active {
    cursor: url(https://feiju12138.github.io/cursor/work.cur), alias
}
p {
    cursor: url(https://feiju12138.github.io/cursor/texto.cur), auto
}
body {
    cursor: url(https://feiju12138.github.io/cursor/normal.cur), auto
}
.cd-top {
    cursor: url(https://feiju12138.github.io/cursor/No_Disponible.cur), auto
}
.ins-section .ins-section-header,.ins-section .ins-search-item {
	cursor: url(https://feiju12138.github.io/cursor/ayuda.cur),auto
}
.ins-section .ins-search-item .ins-search-preview {
	cursor: url(https://feiju12138.github.io/cursor/ayuda.cur),auto
}
.botui-actions-buttons-button {
    cursor: url(https://feiju12138.github.io/cursor/No_Disponible.cur), auto
}
